{
  "name": "Subdued",
  "style": "Dark",
  "author": {
    "name": "Andrew Ford",
    "email": "4DinNZ@gmail.com"
  },

  "editor": {
    "backgroundColor": "#323232",
    "altBackgroundColor": "#292929",
    "tintColor": "#C4B276",
    "tintColor2": "#E9C062",
    "textColor": "#DDD9DBCD",
    "toolbarBackgroundColor": "#323232",
    "toolbarIconColor": "#c5c5c0",
    "menuItemColor": "#c5c5c0",
    "shouldOverwriteFont": true
  },

  "styles": {
    "body": {
      "font": "AvenirNext-Regular",
      "size": 16,
      "color": "#DDD9DBCD"
    },

    "title1": {
      "color": "#CCFDF5DB",
      "size": 34,
      "font": "AvenirNext-DemiBold"
    },

    "title2": {
      "color": "#DDE9C062",
      "size": 24,
      "font": "AvenirNext-DemiBold"
    },

    "title3": {
      "color": "#E9C062",
      "size": 20,
      "font": "AvenirNext-DemiBold"
    },

    "title-mark1": {
      "color": "#66FDF5DB",
      "size": 22,
      "font": "AvenirNext-DemiBold"
    },

    "title-mark2": {
      "color": "#66E9C062",
      "size": 20,
      "font": "AvenirNext-DemiBold"
    },

    "title-mark3": {
      "color": "#66E9C062",
      "size": 16,
      "font": "AvenirNext-DemiBold"
    },

    "bold": {
      "color": "#AACC6666",
      "font": "AvenirNext-Bold"
    },

    "bold-left-mark": {
      "color": "#66CC6666"
    },

    "bold-right-mark": {
      "color": "#66CC6666"
    },

    "italic": {
      "color": "#B56A31",
      "font": "AvenirNext-Italic"
    },

    "italic-left-mark": {
      "color": "#66B56A31"
    },

    "italic-right-mark": {
      "color": "#66B56A31"
    },

    "boldItalic": {
      "font": "AvenirNext-BoldItalic"
    },

    "boldItalic-left-mark": {
      "color": "#66CC6666"
    },

    "boldItalic-right-mark": {
      "color": "#66CC6666"
    },

    "code": {
      "font": "Menlo-Regular",
      "color": "#AACC6666",
      "size": 15
    },

    "code-left-backtick": {
      "color": "#66CC6666"
    },

    "code-right-backtick": {
      "color": "#66CC6666"
    },

    "checked": {
      "headIndent": 33
    },

    "todo": {
      "font": "noteplanstate",
      "color": "#50FFD2A4",
      "headIndent": 33,
      "size": 16
    },

    "checked-todo-characters": {
      "font": "noteplanstate",
      "headIndent": 33,
      "size": 16
    },

    "checked-completed-mark": {
      "regex": "(^\\h*[\\*\\-]{1} |^\\h*[0-9]+[\\.\\)] )(\\[x\\] )",
      "matchPosition": 0,
      "color": "#509DC777",
      "headIndent": 33
    },

    "checked-completed-content": {
      "regex": "(^\\h*[\\*\\-]{1} |^\\h*[0-9]+[\\.\\)] )(\\[x\\] )(.*)",
      "matchPosition": 3,
      "color": "#409DC777",
      "headIndent": 33
    },

    "checked-scheduled-mark": {
      "regex": "(^\\h*[\\*\\-]{1} |^\\h*[0-9]+[\\.\\)] )(\\[\\>\\] )",
      "matchPosition": 0,
      "color": "#40C3AA8A",
      "headIndent": 33
    },

    "checked-scheduled-content": {
      "regex": "(^\\h*[\\*\\-]{1} |^\\h*[0-9]+[\\.\\)] )(\\[\\>\\] )(.*)",
      "matchPosition": 3,
      "color": "#60C3AA8A",
      "headIndent": 33
    },

    "checked-canceled": {
      "regex": "(^\\h*[\\*\\-]{1} |^\\h*[0-9]+[\\.\\)] )(\\[\\-\\] )(.*)",
      "matchPosition": 0,
      "color": "#75CC6666",
      "headIndent": 33
    },

    "special-char": {
      "font": "Menlo-Regular"
    },

    "tabbed": {
      "headIndent": 43
    },

    "quote-mark": {
      "color": "#50FFD2A4",
      "font": "noteplanstate",
      "headIndent": 33,
      "size": 16
    },

    "quote-content": {
      "color": "#B56A31",
      "type": "italic",
      "headIndent": 33,
      "font": "noteplanstate",
      "size": 20
    },

    "highlighted": {
      "regex": "(==)([^:]{1,})(==)",
      "matchPosition": 2,
      "isRevealOnCursorRange": true,
      "backgroundColor": "#60E9C062"
    },

    "highlighted-left-colon": {
      "regex": "(==)([^:]{1,})(==)",
      "matchPosition": 1,
      "isMarkdownCharacter": true,
      "isHiddenWithoutCursor": true,
      "isRevealOnCursorRange": true,
      "color": "#25D9DBCD",
      "backgroundColor": "#60E9C062"
    },

    "highlighted-right-colon": {
      "regex": "(==)([^:]{1,})(==)",
      "matchPosition": 3,
      "isMarkdownCharacter": true,
      "isHiddenWithoutCursor": true,
      "isRevealOnCursorRange": true,
      "color": "#25D9DBCD",
      "backgroundColor": "#60E9C062"
    },

    "flagged-1": {
      "regex": "(^|\\v)(\\h*(\\*|-)\\h*(\\[ \\]\\h+|\\h+)(\\!{1}\\h+.*))($|\\v)",
      "matchPosition": 5,
      "backgroundColor": "#50753636",
      "headIndent": 33
    },

    "flagged-2": {
      "regex": "(^|\\v)(\\h*(\\*|-)\\h*(\\[ \\]\\h+|\\h+)(\\!{2}\\h+.*))($|\\v)",
      "matchPosition": 5,
      "backgroundColor": "#90753636",
      "headIndent": 33
    },

    "flagged-3": {
      "regex": "(^|\\v)(\\h*(\\*|-)\\h*(\\[ \\]\\h+|\\h+)(\\!{3}\\h+.*))($|\\v)",
      "matchPosition": 5,
      "backgroundColor": "#753636",
      "headIndent": 33
    },

    "strikethrough": {
      "regex": "(^|[\\W_])(?:(?!\\1)|(?=^))((\\~|_)\\3)(?=\\S)(.*?\\S)(\\3\\3)(?!\\2)(?=[\\W_]|$)",
      "matchPosition": 4,
      "strikethroughStyle": 1,
      "isRevealOnCursorRange": true,
      "color": "#AAc5c5c0",
      "strikethroughColor": "#CCc5c5c0"
    },

    "strikethrough-left-tilde": {
      "regex": "(^|[\\W_])(?:(?!\\1)|(?=^))((\\~|_)\\3)(?=\\S)(.*?\\S)(\\3\\3)(?!\\2)(?=[\\W_]|$)",
      "matchPosition": 2,
      "isMarkdownCharacter": true,
      "isHiddenWithoutCursor": true,
      "isRevealOnCursorRange": true,
      "color": "#66c5c5c0"
    },

    "strikethrough-right-tilde": {
      "regex": "(^|[\\W_])(?:(?!\\1)|(?=^))(\\~|_)\\2(?=\\S)(.*?\\S)(\\2\\2)(?!\\2)(?=[\\W_]|$)",
      "matchPosition": 4,
      "isMarkdownCharacter": true,
      "isHiddenWithoutCursor": true,
      "isRevealOnCursorRange": true,
      "color": "#66c5c5c0"
    },

    "underline": {
      "regex": "(^|[\\W_])(?:(?!\\1)|(?=^))(\\~|_)(?=\\S)((?:(?!\\2).)*?\\S)(\\2)(?!\\2)(?=[\\W_]|$)",
      "matchPosition": 3,
      "isRevealOnCursorRange": true,
      "underlineStyle": 1,
      "underlineColor": "#AAE9C062"
    },

    "underline-left-tilde": {
      "regex": "(^|[\\W_])(?:(?!\\1)|(?=^))(\\~|_)(?=\\S)((?:(?!\\2).)*?\\S)(\\2)(?!\\2)(?=[\\W_]|$)",
      "matchPosition": 2,
      "isMarkdownCharacter": true,
      "isHiddenWithoutCursor": true,
      "isRevealOnCursorRange": true,
      "color": "#44E9C062"
    },

    "underline-right-tilde": {
      "regex": "(^|[\\W_])(?:(?!\\1)|(?=^))(\\~|_)(?=\\S)((?:(?!\\2).)*?\\S)(\\2)(?!\\2)(?=[\\W_]|$)",
      "matchPosition": 4,
      "isMarkdownCharacter": true,
      "isHiddenWithoutCursor": true,
      "isRevealOnCursorRange": true,
      "color": "#44E9C062"
    },

    "link": {},

    "schedule-to-date-link": {},

    "done-date": {},

    "schedule-from-date-link": {},

    "note-title-link": {},

    "hashtag": {},

    "attag": {},

    "phonenumber": {}
  }
}
